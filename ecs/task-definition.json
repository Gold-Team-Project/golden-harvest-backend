{
  "family": "harvest-task",
  "taskRoleArn": "arn:aws:iam::686255985190:role/harvest-ecs-task-role",
  "executionRoleArn": "arn:aws:iam::686255985190:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "containerDefinitions": [
    {
      "name": "test-container",
      "image": "686255985190.dkr.ecr.ap-northeast-2.amazonaws.com/gold/harvest:PLACEHOLDER",
      "cpu": 0,
      "portMappings": [
        {
          "name": "test-container-port-protocol",
          "containerPort": 8088,
          "hostPort": 8088,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "secrets": [
        { "name": "ACCESS_KEY_ID", "valueFrom": "arn:aws:ssm:ap-northeast-2:686255985190:parameter/ACCESS_KEY_ID" },
        { "name": "SECRET_ACCESS_KEY", "valueFrom": "arn:aws:ssm:ap-northeast-2:686255985190:parameter/SECRET_ACCESS_KEY" },
        { "name": "KAMIS_CERT_ID", "valueFrom": "arn:aws:ssm:ap-northeast-2:686255985190:parameter/KAMIS_CERT_ID" },
        { "name": "KAMIS_CERT_KEY", "valueFrom": "arn:aws:ssm:ap-northeast-2:686255985190:parameter/KAMIS_CERT_KEY" },
        { "name": "MAIL_USERNAME", "valueFrom": "arn:aws:ssm:ap-northeast-2:686255985190:parameter/MAIL_USERNAME" },
        { "name": "MAIL_PASSWORD", "valueFrom": "arn:aws:ssm:ap-northeast-2:686255985190:parameter/MAIL_PASSWORD" },
        { "name": "AMAZON_RDS", "valueFrom": "arn:aws:ssm:ap-northeast-2:686255985190:parameter/AMAZON_RDS" },
        { "name": "JWT_SECRET_KEY", "valueFrom": "arn:aws:ssm:ap-northeast-2:686255985190:parameter/JWT_SECRET_KEY" },
        { "name": "REDIS_HOST", "valueFrom": "arn:aws:ssm:ap-northeast-2:686255985190:parameter/REDIS_HOST" },
        { "name": "REDIS_PORT", "valueFrom": "arn:aws:ssm:ap-northeast-2:686255985190:parameter/REDIS_PORT" }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/harvest-task",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
